{
  "name": "cai-island",
  "version": "1.0.5",
  "description": "Vite SSG framework based on islands arch",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup && tsx ./scripts/prebundle.ts",
    "build:deps": "tsx ./scripts/prebundle.ts",
    "preview": "cd docs/build && serve .",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js,.jsx --quiet ./",
    "prepare": "husky install",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "prepare:e2e": "tsx scripts/prepare-e2e.cts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "tsx ./scripts/release.ts"
  },
  "bin": {
    "island": "bin/island.js"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts}": [
      "npm run lint:fix"
    ]
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "src/runtime",
    "src/theme-default",
    "src/shared",
    "template.html"
  ],
  "keywords": [
    "vite",
    "ssg"
  ],
  "author": "cjhw",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@playwright/test": "1.26.1",
    "@types/babel__core": "^7.20.0",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/babel__traverse": "^7.18.3",
    "@types/fs-extra": "^9.0.13",
    "@types/hast": "^2.3.4",
    "@types/mdast": "^3.0.10",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/resolve": "^1.20.2",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitest/ui": "^0.25.3",
    "chalk": "^5.2.0",
    "commitlint": "^17.3.0",
    "enquirer": "^2.3.6",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "execa": "5.1.1",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "minimist": "^1.2.8",
    "rollup": "^3.4.0",
    "sass": "^1.58.0",
    "semver": "^7.3.8",
    "tsup": "^6.5.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.3",
    "unist-util-visit": "^4.1.1",
    "vitest": "^0.25.3"
  },
  "dependencies": {
    "@babel/core": "^7.21.0",
    "@babel/helper-plugin-utils": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/traverse": "^7.21.2",
    "@iconify-json/carbon": "^1.1.14",
    "@loadable/component": "^5.15.2",
    "@mdx-js/rollup": "^2.2.1",
    "@types/lodash-es": "^4.17.6",
    "@vitejs/plugin-react": "^2.2.0",
    "acorn": "^8.8.1",
    "cac": "^6.7.14",
    "compression": "^1.7.4",
    "esbuild": "^0.17.11",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "github-slugger": "^2.0.0",
    "hast-util-from-html": "^1.0.0",
    "lodash-es": "^4.17.21",
    "mdast-util-mdxjs-esm": "^1.3.0",
    "ora": "^6.1.2",
    "polka": "^0.5.2",
    "prettier": "^2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.4.5",
